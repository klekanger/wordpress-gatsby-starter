{
  "name": "wordpress-gatsby-starter",
  "private": true,
  "description": "A simple starter for using Wordpress as a headless CMS with Gatsby",
  "version": "0.2.0",
  "author": "Kurt Lekanger <kurt@lekanger.no>",
  "dependencies": {
    "@wordpress/block-library": "^2.22.6",
    "dotenv": "^8.2.0",
    "gatsby": "^5.5.0",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-manifest": "^2.4.23",
    "gatsby-plugin-offline": "^3.2.23",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-source-graphql": "^2.7.1",
    "gatsby-source-wordpress-experimental": "^1.3.6",
    "gatsby-transformer-sharp": "^2.5.13",
